    <div class="row">
        <dashboard-stat
                description="{{ 'dashboard.book_models.count.description' | trans }}"
                viewMore="{{ 'dashboard.view_more' | trans }}"
                link="{{ path('home_index') }}"
                color="red"
                icon="fa fa-shopping-cart"
                url="http://lms.com/api/book_models/count"
        ></dashboard-stat>

        <dashboard-stat
            description="{{ 'dashboard.book_rents.count.description' | trans }}"
            viewMore="{{ 'dashboard.view_more' | trans }}"
            link="{{ path('home_index') }}"
            color="purple"
            icon="fa fa-shopping-cart"
            url="http://lms.com/api/book_models/count"
        ></dashboard-stat>

        <dashboard-stat
            description="{{ 'dashboard.book_bookins.count.description' | trans }}"
            viewMore="{{ 'dashboard.view_more' | trans }}"
            link="{{ path('home_index') }}"
            color="green"
            icon="fa fa-shopping-cart"
            url="http://lms.com/api/book_models/count"
        ></dashboard-stat>
