{% extends "base.html.twig" %}

{% trans_default_domain 'user' %}

{% block page_level_header_plugins %}
<!-- BEGIN PAGE LEVEL PLUGINS -->
<link href="https://keenthemes.com/preview/metronic/theme/assets/global/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" />
<link href="https://keenthemes.com/preview/metronic/theme/assets/global/plugins/select2/css/select2-bootstrap.min.css" rel="stylesheet" type="text/css" />
<!-- END PAGE LEVEL PLUGINS -->
{% endblock page_level_header_plugins %}

{% block page_level_styles %}
<!-- BEGIN PAGE LEVEL STYLES -->
<link href="https://keenthemes.com/preview/metronic/theme/assets/pages/css/login.min.css" rel="stylesheet" type="text/css" />
<!-- END PAGE LEVEL STYLES -->
{% endblock page_level_styles %}


{% block body_class ' login' %}

{% block body_content %}
    <!-- BEGIN LOGO -->
    <div class="logo">
        {% if false %}
        <a href="{{ path('home_index') }}">
            {% if false %}
            <img src="https://keenthemes.com/preview/metronic/theme/assets/pages/img/logo-big.png" alt="" />
            {% endif %}
        </a>
        {% else %}
            <h3 style="color: white"><b>LMS</b> | Library Management System</h3>
        {% endif %}
    </div>
    <!-- END LOGO -->

    <!-- BEGIN LOGIN -->
    <div class="content">
        <!-- BEGIN LOGIN FORM -->
        {{ form_start(login_form, { 'attr' : { 'class' : 'login-form' } }) }}
            <h3 class="form-title font-green">{{ 'user.login.title' | trans }}</h3>

            {% if login_error %}
            <div class="alert alert-danger display-hide_">
                <button class="close" data-close="alert"></button>
                <span> {{ login_error.messageKey|trans(login_error.messageData, 'security') }} </span>
            </div>
            {% endif %}

            <div class="form-group">
                {{
                    form_label(
                        login_form.email,
                        'user.login.form.input.email.label' | trans,
                        {
                            'label_attr': {
                                'class': 'control-label visible-ie8 visible-ie9'
                            }
                        }
                    )
                }}

                {{
                    form_widget(
                        login_form.email,
                        {
                            'attr': {
                            'class': 'form-control form-control-solid placeholder-no-fix',
                            'placeholder': 'user.login.form.input.email.placeholder' | trans,
                            'autocomplete': 'off',
                            }
                        }
                    )
                }}
            </div>

            <div class="form-group">
                {{
                    form_label(
                        login_form.password,
                        'user.login.form.input.password.label' | trans,
                        {
                            'label_attr': {
                                'class': 'control-label visible-ie8 visible-ie9'
                            }
                        }
                    )
                }}

                {{
                    form_widget(
                        login_form.password,
                        {
                            'attr': {
                                'class': 'form-control form-control-solid placeholder-no-fix',
                                'placeholder': 'user.login.form.input.password.placeholder' | trans,
                                'autocomplete': 'off',
                            }
                        }
                    )
                }}
            </div>

            <div class="form-actions">
                {{
                    form_widget(
                        login_form.submit,
                        {
                            'attr': {
                                'class': 'btn green uppercase',
                                'id': 'm_login_signin_submit',
                                'type': 'submit',
                            },

                            'label': 'user.login.form.button.submit.label' | trans,
                        }
                    )
                }}

                {% if false %}
                <label class="rememberme check mt-checkbox mt-checkbox-outline">
                    <input type="checkbox" name="remember" value="1" />
                    {{ 'user.login.form.label.remember' | trans }}

                    <span></span>
                </label>
                {% endif %}

                {% if false %}
                <a href="javascript:;" id="forget-password" class="forget-password">Forgot Password?</a>
                {% endif %}
            </div>

            {% if false %}
            <div class="login-options">
                <h4>Or login with</h4>
                <ul class="social-icons">
                    <li>
                        <a class="social-icon-color facebook" data-original-title="facebook" href="javascript:;"></a>
                    </li>
                    <li>
                        <a class="social-icon-color twitter" data-original-title="Twitter" href="javascript:;"></a>
                    </li>
                    <li>
                        <a class="social-icon-color googleplus" data-original-title="Goole Plus" href="javascript:;"></a>
                    </li>
                    <li>
                        <a class="social-icon-color linkedin" data-original-title="Linkedin" href="javascript:;"></a>
                    </li>
                </ul>
            </div>
            {% endif %}

            {% if false %}
            <div class="create-account">
                <p>
                    <a href="javascript:;" id="register-btn" class="uppercase">Create an account</a>
                </p>
            </div>
            {% endif %}
        {{ form_end(login_form) }}
        <!-- END LOGIN FORM -->
        <!-- BEGIN FORGOT PASSWORD FORM -->
        <form class="forget-form" action="{{ path('home_index') }}" method="post">
            <h3 class="font-green">Forget Password ?</h3>
            <p> Enter your e-mail address below to reset your password. </p>
            <div class="form-group">
                <input class="form-control placeholder-no-fix" type="text" autocomplete="off" placeholder="Email" name="email" /> </div>
            <div class="form-actions">
                <button type="button" id="back-btn" class="btn green btn-outline">Back</button>
                <button type="submit" class="btn btn-success uppercase pull-right">Submit</button>
            </div>
        </form>
